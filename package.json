{
  "name": "prisma-mapper",
  "version": "1.1.1",
  "description": "A CLI that adds @Map and @@Map based on a json for the prisma schema",
  "main": "bin/index.js",
  "preferGlobal": true,
  "bin": {
    "prisma-mapper": "bin/commands.js"
  },
  "types": "bin",
  "files": [
    "bin/*"
  ],
  "repository": "https://github.com/RaresAil/prisma-mapper.git",
  "author": "RaresAil <ailincairares1@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev:bin": "yarn build && NODE_ENV=dev node bin/commands.js",
    "dev": "NODE_ENV=dev nodemon src/commands.ts",
    "build": "yarn lint && rm -rf ./bin/ && tsc -p .",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "@prisma/internals": "^4.3.1",
    "colors": "^1.4.0",
    "commander": "^9.4.0",
    "inquirer": "^9.1.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.1",
    "@types/node": "^18.7.15",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.0.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-security": "^1.5.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "*"
  }
}
